{
  "name": "fasmext",
  "license": "MIT",
  "icon": "icons/fasm.png",
  "publisher": "MuhinBetenyaInc",
  "displayName": "FASM extension",
  "description": "FASM language extension for visual studio code",
  "version": "0.0.5",
  "repository": {
    "type": "git",
    "url": "https://github.com/kostyabet/VSCode-FASM-ext.git"
  },
  "engines": {
    "vscode": "^1.96.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:fasm",
    "onFileSystem:*.asm"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "themes": [
      {
        "label": "FASM Theme",
        "path": "./themes/fasmext.themes.json"
      }
    ],
    "languages": [
      {
        "id": "fasm",
        "extensions": [
          ".asm"
        ],
        "aliases": [
          "fasm",
          "FlatASM"
        ],
        "filenames": [],
        "icon": {
          "light": "./icons/fasm.png",
          "dark": "./icons/fasm.png"
        }
      }
    ],
    "grammars": [
      {
        "language": "fasm",
        "scopeName": "fasmext.instractions",
        "path": "./syntaxes/instractions.tmGrammar.json"
      }
    ],
    "commands": [
      {
        "command": "extension.highlightLoops",
        "title": "Highlight Loops"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test",
    "deploy": "vsce publish --yarn"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.96.0",
    "@typescript-eslint/eslint-plugin": "^8.17.0",
    "@typescript-eslint/parser": "^8.17.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "@vscode/vsce": "^3.2.1",
    "eslint": "^9.16.0",
    "ts-loader": "^9.5.1",
    "typescript": "^5.7.2",
    "webpack": "^5.95.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "yarn": "^1.22.22"
  }
}
